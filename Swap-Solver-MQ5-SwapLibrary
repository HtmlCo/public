//| Script program start function                                    |
//+------------------------------------------------------------------+
void OnStart()
  {
//---
 Print ("Swap Solver");
 Print("====================================");
double swapRate=0;
string asymbol="USD_JPY";

swapRate=SymbolInfoDouble(_Symbol,SYMBOL_SWAP_LONG); 
Print(   swapRate );
Print("====================================");



//Print("==== Calculate Swap =================");
asymbol="EUR_USD";
Print(asymbol +", " +   swapRate );
swapRate=SymbolInfoDouble(asymbol,SYMBOL_SWAP_LONG); 

   
  }
//+------------------------------------------------------------------+
